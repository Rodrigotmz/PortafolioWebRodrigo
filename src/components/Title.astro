---
type Props = {
  title?: string;
};

const { title } = Astro.props;
---

<div class="portafolio-row">
  <div class="padd-15 sm:mb-[60px] max-w-full flex-shrink-0 flex-grow-0 basis-full">
    <h2 class="text-white-primary title-before title-after relative sm:text-4xl font-bold text-2xl">
      {title}
    </h2>
  </div>
</div>

<style></style>
